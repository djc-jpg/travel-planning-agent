{
  "id": null,
  "uid": "trip-agent-overview",
  "title": "Trip Agent Overview",
  "tags": ["trip-agent", "slo"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-30m",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Success Rate",
      "gridPos": {"h": 6, "w": 6, "x": 0, "y": 0},
      "targets": [
        {"expr": "trip_agent_success_rate", "refId": "A"}
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": null},
              {"color": "orange", "value": 0.95},
              {"color": "green", "value": 0.99}
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {"values": false, "calcs": ["lastNotNull"], "fields": ""},
        "orientation": "auto",
        "textMode": "value",
        "colorMode": "value"
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "P95 Latency (ms)",
      "gridPos": {"h": 6, "w": 6, "x": 6, "y": 0},
      "targets": [
        {"expr": "trip_agent_latency_p95_ms", "refId": "A"}
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "orange", "value": 2000},
              {"color": "red", "value": 3000}
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {"values": false, "calcs": ["lastNotNull"], "fields": ""},
        "orientation": "auto",
        "textMode": "value",
        "colorMode": "value"
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Request Volume",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
      "targets": [
        {"expr": "trip_agent_requests_total", "refId": "A"}
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        },
        "overrides": []
      },
      "options": {
        "legend": {"displayMode": "table", "placement": "bottom"},
        "tooltip": {"mode": "single"}
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Tool Error Ratio",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
      "targets": [
        {
          "expr": "sum(trip_agent_tool_calls_total{status=\"error\"}) / clamp_min(sum(trip_agent_tool_calls_total), 1)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit"
        },
        "overrides": []
      },
      "options": {
        "legend": {"displayMode": "table", "placement": "bottom"},
        "tooltip": {"mode": "single"}
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Degrade Mix by Level",
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 14},
      "targets": [
        {"expr": "trip_agent_degrade_level_total{degrade_level=\"L0\"}", "refId": "A", "legendFormat": "L0"},
        {"expr": "trip_agent_degrade_level_total{degrade_level=\"L1\"}", "refId": "B", "legendFormat": "L1"},
        {"expr": "trip_agent_degrade_level_total{degrade_level=\"L2\"}", "refId": "C", "legendFormat": "L2"},
        {"expr": "trip_agent_degrade_level_total{degrade_level=\"L3\"}", "refId": "D", "legendFormat": "L3"}
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        },
        "overrides": []
      },
      "options": {
        "legend": {"displayMode": "table", "placement": "bottom"},
        "tooltip": {"mode": "multi"}
      }
    }
  ],
  "templating": {
    "list": []
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {"type": "grafana", "uid": "-- Grafana --"},
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  }
}
